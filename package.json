{
  "name": "kartik-jha-portfolio",
  "version": "1.0.0",
  "private": true,
  "description": "Minimalist portfolio website for Kartik Jha - Full Stack Developer",
  "keywords": ["portfolio", "react", "developer", "full-stack"],
  "author": "Kartik Jha",
  "license": "MIT",
  "homepage": "https://kartikjha.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/KartikJha/kartikjha.github.io"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "lucide-react": "^0.263.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.45.0",
    "eslint-config-react-app": "^7.0.1",
    "prettier": "^3.0.0",
    "gh-pages": "^5.0.0",
    "web-vitals": "^3.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
